{
  "services": [
    {
      "id": "mycms-frontend",
      "name": "mycms.space Frontend",
      "image": "mycms-space:latest",
      "ports": [
        {
          "containerPort": 80,
          "protocol": "tcp",
          "hostPort": 80
        }
      ],
      "env": [
        {
          "key": "VITE_SUPABASE_URL",
          "value": "${VITE_SUPABASE_URL}"
        },
        {
          "key": "VITE_SUPABASE_ANON_KEY",
          "value": "${VITE_SUPABASE_ANON_KEY}"
        }
      ],
      "volumes": [],
      "networks": [
        "mycms-network"
      ],
      "restart": "always",
      "healthCheck": {
        "test": ["CMD", "wget", "--no-verbose", "--tries=1", "--spider", "http://localhost/"],
        "interval": 30,
        "timeout": 3,
        "retries": 3
      }
    }
  ],
  "networks": [
    {
      "id": "mycms-network",
      "name": "mycms-network",
      "driver": "bridge"
    }
  ]
}
